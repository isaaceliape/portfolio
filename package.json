{
  "name": "sclp",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.babel.js",
  "directories": {
    "doc": "doc"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "start": "gulp",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "deploy": "gulp deploy && sh deploy.sh && npm run success",
    "success": "osascript -e 'display notification \"All files were uploaded with success\" with title \"Deploy done\"'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "makepdf": "mdpdf isaac_eliape_cv.md"
  },
  "keywords": [
    "website"
  ],
  "author": "Isaac Eliape",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.26.3",
    "browserify": "^13.0.1",
    "critical": "^0.7.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-loader": "^0.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-env": "^0.4.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.12.0",
    "gulp-uglify": "^1.5.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "gsap": "^1.20.2",
    "gyronorm": "^2.0.6",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "masonry-layout": "^4.2.0",
    "moment": "^2.18.1",
    "page.js": "^4.13.3",
    "pixi.js": "^4.8.2"
  }
}
